// LINK 

// QUESTION  

/*
LINEAR SEARCH

*/

// SOLUTION
#include <iostream>
using namespace std; 
int linear(int a[],int n,int x)
{
    for(int i=0;i<n;i++)
    {
        if(a[i]==x)
        {
            return i;
            break;
        }
    }
    return -1;
}
int main() {
int n,x;
cin>>n>>x;
 int a[n];
 for(int i=0;i<n;i++)
 {
    cin>>a[i];
 }
cout<<linear(a,n,x);
    }
